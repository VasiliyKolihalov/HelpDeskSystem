{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Server=postgres_db;Port=5432;Database=usersdb;User Id=postgres;Password={optional_placeholder_that_should_be_same_as_in_docker-compose}",
    "Master": "Server=postgres_db;Port=5432;Database=postgres;User Id=postgres;Password={optional_placeholder_that_should_be_same_as_in_docker-compose}"
  },
  "RabbitMqOptions": {
    "Host": "rabbit_mq",
    "Port": "5672",
    "UserName": "rabbitmq",
    "Password": "{optional_placeholder_that_should_be_same_as_in_docker-compose}",
    "AutomaticRecovery": true,
    "ExchangeName": "users",
    "ConnectionRetryCount": 5,
    "ConnectionRetrySleepDuration": "0:00:05"
  },
  "JwtAuthOptions": {
    "Secret": "{optional_placeholder_that_is_long_enough_for_sha256}",
    "TokenLifetime": "0:20:00"
  }
}
